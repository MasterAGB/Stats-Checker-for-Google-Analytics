{
    "background":{
      "page":"background.html"
    },
    "browser_action":{
      "default_icon":"img/logo.32.png",
      "default_title":"__MSG_extName__",
      "default_popup":"popup.html"
    },

    "default_locale":"en",
    "name":"__MSG_extName__",
    "short_name":"__MSG_extShortName__",
    "description":"__MSG_extDesc__",
    "icons":{
        "16":"img/logo.16.png",
        "32":"img/logo.32.png",
        "48":"img/logo.48.png",
        "64":"img/logo.64.png",
        "128":"img/logo.128.png"
    },
    "options_page":"options.html",
    "permissions":[
        "https://analytics.google.com/*"
    ],
    "version":"1.6.5.0",
    "content_security_policy":"script-src 'self' https://connect.facebook.net https://ssl.google-analytics.com https://apis.google.com https://platform.twitter.com; object-src 'self'",
    "manifest_version":2
}
